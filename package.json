{
  "name": "rest-server",
  "version": "1.0.0",
  "description": "A mock backend api json-server",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "json-server --watch db.json --routes routes.json --port 4000 --static ./public --middlewares ./middleware.js",
    "js": "json-server --watch db.js --routes routes.json --port 4000 --static ./public --middlewares ./middleware.js",
    "chalet-start": "chalet add 'npm start' -p 4000 && chalet start",
    "chalet-stop": "chalet stop",
    "open-browser": "google-chrome --proxy-pac-url=http://localhost:2000/proxy.pac"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "casual": "^1.6.2",
    "chalet": "^0.10.1",
    "chance": "^1.1.8",
    "json-server": "^0.17.0"
  }
}
